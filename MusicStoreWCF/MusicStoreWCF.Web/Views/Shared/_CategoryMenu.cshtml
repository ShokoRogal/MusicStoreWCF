@model List<MusicStoreWCF.Web.Models.Category>
<div class="col-md-3">
    <p class="lead"></p>
    <div class="list-group">
        @foreach (var category in Model)
            {
            @Html.ActionLink(category.Name, "CategoryProduct", "Home", new { categoryId = category.CategoryId}, new { @class = "list-group-item" })
        }
    </div>
</div>